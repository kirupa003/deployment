<?xml version="1.0"?>
<!-- {{ ansible_managed }} -->
<clickhouse>
    <users>
        <{{ clickhouse_user }}>
            <password>{{ clickhouse_password }}</password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </{{ clickhouse_user }}>
    </users>
</clickhouse>
